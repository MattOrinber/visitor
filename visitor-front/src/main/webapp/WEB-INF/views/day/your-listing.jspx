<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:security="http://www.springframework.org/security/tags" xmlns:form="http://www.springframework.org/tags/form" xmlns:spring="http://www.springframework.org/tags">

<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />

<body>
	<c:if test="${not empty loginUserEmail}">
		<script type="text/javascript">ifLogginIn = 1;</script>
	</c:if>
	
<![CDATA[
	<script>
		$(document).ready(function(){
			initLoginBar();
			setGlobalCurrency();
			startChecking();
		});
	</script>
	
<div class="nav">
	<div class="nav-detail">
    	<a href="${pageContext.request.contextPath}/day/dashboard">Dashboard</a>
        <a href="${pageContext.request.contextPath}/day/inbox">Inbox</a>
        <a href="javascript:void(0);" class="select">Your Listings</a>
        <a href="javascript:void(0);">Your Inquiries</a>
        <a href="${pageContext.request.contextPath}/day/edit">Edit Profile</a>
    </div>
</div>

<div class="main">
    <div class="maincontent">
         <div class="inbox">
            	<span>
                	<select>
                    	<option>Show all listings</option>
                        <option>Show active</option>
                        <option>Show hidden</option>
                    </select>
                </span>
]]>
				<c:if test="${not empty productList}">
					<c:forEach items="${productList}" var="productT" varStatus="status">
<![CDATA[
						<div class="message">
	                        <div class="photo photo1"></div>
	                        <div class="time" style="width:300px;">
]]>
							<c:if test="${productT.productStatus == 0}">
	                        	<a href="${pageContext.request.contextPath}/day/calendar?pid=${productT.productId}" class="title">${productT.productRoomType} in ${productT.productCity}</a>
	                        </c:if>
	                        
	                        <c:if test="${productT.productStatus != 0}">
	                        	<a href="${pageContext.request.contextPath}/day/product?pid=${productT.productId}" class="title">${productT.productRoomType} in ${productT.productCity}</a>
	                        </c:if>
<![CDATA[
	                            <a href="${pageContext.request.contextPath}/day/calendar?pid=${productT.productId}" class="calendar">Manage Listing and Calendar</a>
	                        </div>
	                        <div class="price" style="width:200px;">
]]>
								<c:if test="${productT.productStatus == 1}">
<![CDATA[
									<a href="javascript:void(0);" class="dot dot1"></a>
									<select>
										<option>Unlisted</option>
										<option selected="selected">listed</option>
									</select>
]]>
								</c:if>
								
								<c:if test="${productT.productStatus == 2}">
<![CDATA[
									<a href="javascript:void(0);" class="dot"></a>
									<select>
										<option selected="selected">Unlisted</option>
										<option>listed</option>
									</select>
]]>
								</c:if>
								
								<c:if test="${productT.productStatus == 0}">
									<a href="${pageContext.request.contextPath}/day/calendar?pid=${productT.productId}" class="btn">6 steps to list</a>
								</c:if>
<![CDATA[     
	                        </div>
	                   </div>
]]>
					</c:forEach>
				</c:if>
<![CDATA[
         </div>
    </div>
</div>

]]>
</body>
</html>
