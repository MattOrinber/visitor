<script>
	$(document).ready(function(){
		
		  $("#signbtn").click(function(){
			  		$('.wrapwrapbox').show();
			  		$("#signup").show();			  
			  });
			  
		  $("#logbtn").click(function(){
			  		$('.wrapwrapbox').show();
			  		$("#login").show();			  
			  });
		  $("#signupTologin").click(function(){
			  		$("#signup").hide();
			  		$("#login").show();			  
			  });
		  $("#loginTosignup").click(function(){
			  		$("#login").hide();
			  		$("#signup").show();			  
			  });  
		  $(".wrapwrapbox").click(function(){
			  		$(this).hide();
			  		$("#signup").hide();
					$("#login").hide();			  
			  });
			  
		
		  $(".img").mouseenter(function(){
			  	$(this).find(".btn").show();
			  });
		  $(".img").mouseleave(function(){
			  	$(this).find(".btn").hide();
			  });  
		  
		  $("#ScrolltoTop").click(function() {
				$('html, body').animate({
					scrollTop: 0
				},
				"normal")
			})
		  
	});
	
	//banner change
	$(function(){
		var aImg = $('.img ul li');		
		var iSize = aImg.size();		
		var index = 0;		
		var t;
		$('#btnLeft').click(function(){		//左边按钮点击
			index--;
			if(index<0){
				index=iSize-1
			}
			change(index)
		})
		$('#btnRight').click(function(){    //右边按钮点击
			index++;
			if(index>iSize-1){
				index=0
			}
			change(index)
		})
		//切换过程
		function change(index){
			//aPage.removeClass('active');
			//aPage.eq(index).addClass('active');
			aImg.stop();
			//隐藏除了当前元素，所以图像
			aImg.eq(index).siblings().animate({
				opacity:0
			},100)
			//显示当前图像
			aImg.eq(index).animate({
				opacity:1
			},100)
		}
	})


</script>
