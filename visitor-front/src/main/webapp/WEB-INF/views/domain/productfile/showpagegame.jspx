<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" 
xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" 
xmlns:form="http://www.springframework.org/tags/form" xmlns:security="http://www.springframework.org/security/tags" 
xmlns:spring="http://www.springframework.org/tags" version="2.0">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />
<head>
	<title><spring:message code="productFile" />: <spring:message code="crud.functionalities.show" /></title>
</head>
<body>

	<a href="${pageContext.request.contextPath}/domain/productfile/search" class="ajaxy button button-search"><spring:message code="productFile" /></a>
	<a href="${pageContext.request.contextPath}/domain/productlist/show/${productFile.product.primaryKey}" class="ajaxy button button-search"><spring:message code="productList" /></a>
	<span class="ui-state-highlight button button-show"><spring:message code="crud.show.button" /></span>
	<a href="${pageContext.request.contextPath}/domain/productfile/updatepagegame/${productFile.primaryKey}" class="ajaxy button button-edit"><spring:message code="crud.edit.button" /></a>
	<a href="${pageContext.request.contextPath}/domain/productfile/delete/${productFile.primaryKey}" class="modal button button-delete"><spring:message code="crud.delete.button" /></a>

	<table class="show">
		<tbody>
			<tr>
				<th><spring:message code="productFile_SoupengName" /></th>
				<td>
					<spring:eval expression="productFile.fileName"/>
				</td>
				<th><spring:message code="productFile_product" /> </th>
				<td>
					<a href="${pageContext.request.contextPath}/domain/productlist/show/${productFile.product.primaryKey}"><spring:eval expression="productFile.product.name" /></a>
				</td>
			</tr>
			<tr>
				<th><spring:message code="productFile_PageGameUrl" /></th>
				<td><spring:eval expression="productFile.fileUrl"/></td>
				<th><spring:message code="productFile_SoupengDisName" /></th>
				<td><spring:eval expression="productFile.fileDisName"/></td>
			</tr>
			<tr>
				<th><spring:message code="productFile_SoupengId" /> </th>
				<td><spring:eval expression="productFile.filePath" /></td>
				<th><spring:message code="productFile_status" /></th>
				<td>
					<c:choose>
						<c:when test="${productFile.status == 0}">
							<spring:message code="Available" />
						</c:when>
						<c:otherwise>
							<spring:message code="UnAvailable" />
						</c:otherwise>
					</c:choose>
				</td>
			</tr>
			<tr>
				<th><spring:message code="productFile_createBy" /></th>
				<td><spring:eval expression="productFile.createBy"/></td>
				<th><spring:message code="productFile_createDate" /></th>
				<td><spring:eval expression="productFile.createDate"/></td>
			</tr>
			<tr>
				<th><spring:message code="productFile_modBy" /></th>
				<td><spring:eval expression="productFile.modBy"/></td>
				<th><spring:message code="productFile_modDate" /></th>
				<td><spring:eval expression="productFile.modDate"/></td>
			</tr>
		</tbody>
	</table>
</body>
</html>