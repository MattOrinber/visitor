<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" 
xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" 
xmlns:form="http://www.springframework.org/tags/form" xmlns:security="http://www.springframework.org/security/tags" 
xmlns:spring="http://www.springframework.org/tags" version="2.0">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />
<head>
	<title><spring:message code="messageReply" />: <spring:message code="crud.functionalities.show" /></title>
</head>
<body>

	<a href="${pageContext.request.contextPath}/domain/messagereply/search" class="ajaxy button button-search"><spring:message code="messageReply" /></a>
	<span class="ui-state-highlight button button-show"><spring:message code="crud.show.button" /></span>
	<a href="${pageContext.request.contextPath}/domain/messagereply/update/${messageReply.primaryKey}" class="ajaxy button button-edit"><spring:message code="crud.edit.button" /></a>
	<a href="${pageContext.request.contextPath}/domain/messagereply/delete/${messageReply.primaryKey}" class="modal button button-delete"><spring:message code="crud.delete.button" /></a>

	<table class="show">
		<tbody>
			<tr>
				<th><spring:message code="messageReply_messageId" /></th>
				<td>
					<spring:eval expression="messageReply.message" />
					<a href="${pageContext.request.contextPath}/domain/messagecontent/show/${messageReply.message.primaryKey}" class="button button-show miniature"><spring:message code="crud.show.button" /></a>
				</td>
			</tr>
			<tr>
				<th><spring:message code="messageReply_num" /></th>
				<td><spring:eval expression="messageReply.num"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageReply_createDate" /></th>
				<td><spring:eval expression="messageReply.createDate"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageReply_createBy" /></th>
				<td><spring:eval expression="messageReply.createBy"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageReply_status" /></th>
				<td><spring:eval expression="messageReply.status"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageReply_content" /></th>
				<td><spring:eval expression="messageReply.content"/></td>
			</tr>
		</tbody>
	</table>
</body>
</html>