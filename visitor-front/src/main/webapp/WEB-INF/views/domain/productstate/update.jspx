<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" 
xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" 
xmlns:form="http://www.springframework.org/tags/form" xmlns:security="http://www.springframework.org/security/tags" 
xmlns:spring="http://www.springframework.org/tags" version="2.0">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />
<head>
	<title><spring:message code="productState" />: <spring:message code="crud.functionalities.update" /></title>
</head>
<body>
	<a href="${pageContext.request.contextPath}/domain/productstate/search" class="ajaxy button button-search"><spring:message code="productState" /></a>
	<a href="${pageContext.request.contextPath}/domain/productstate/show/${productState.primaryKey}" class="ajaxy button button-show"><spring:message code="crud.show.button" /></a>
	<span class="ui-state-highlight button button-edit"><spring:message code="crud.edit.button" /></span>
	<div id="update">
	<form:form action="${pageContext.request.contextPath}/domain/productstate/update/${productState.primaryKey}" modelAttribute="productState" method="POST">
		<form:errors cssClass="error"/>
		<table class="edit">
			<tbody>
				<tr>
					<th><spring:message code="productState_totalDl" /></th>
					<td>
						<form:input path="totalDl" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="totalDl" cssClass="error"/>
					</td>
					<th><spring:message code="productState_totalPv" /></th>
					<td>
						<form:input path="totalPv" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="totalPv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_totalUv" /></th>
					<td>
						<form:input path="totalUv" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="totalUv" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="productState_dailyDl" /></th>
					<td>
						<form:input path="dailyDl" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="dailyDl" cssClass="error"/>
					</td>
					<th><spring:message code="productState_dailyPv" /></th>
					<td>
						<form:input path="dailyPv" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="dailyPv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_dailyUv" /></th>
					<td>
						<form:input path="dailyUv" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="dailyUv" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="productState_weekDl" /></th>
					<td>
						<form:input path="weekDl" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="weekDl" cssClass="error"/>
					</td>
					<th><spring:message code="productState_weekPv" /></th>
					<td>
						<form:input path="weekPv" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="weekPv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_weekUv" /></th>
					<td>
						<form:input path="weekUv" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="weekUv" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="productState_monthDl" /></th>
					<td>
						<form:input path="monthDl" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="monthDl" cssClass="error"/>
					</td>
					<th><spring:message code="productState_monthPv" /></th>
					<td>
						<form:input path="monthPv" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="monthPv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_monthUv" /></th>
					<td>
						<form:input path="monthUv" cssClass="number required" min="0" maxlength="18"/>
						<form:errors path="monthUv" cssClass="error"/>
					</td>
				</tr>
			</tbody>
			<tfoot>
				<tr>
					<td colspan="6">
						<spring:message code="crud.save.button" var="saveButton"/>
						<input type="submit" class="button button-save" value="${saveButton}" />
					</td>
				</tr>
			</tfoot>
		</table>
	</form:form>
	</div>
</body>
</html>