<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" 
xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" 
xmlns:form="http://www.springframework.org/tags/form" xmlns:security="http://www.springframework.org/security/tags" 
xmlns:spring="http://www.springframework.org/tags" version="2.0">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />
<head>
	<title><spring:message code="productState" />: <spring:message code="crud.functionalities.example" /></title>
</head>
<body>
	<span class="ui-state-highlight button button-search"><spring:message code="productState" /></span> 
	<form:form action="${pageContext.request.contextPath}/domain/productstate/list" modelAttribute="productStateSearchForm" class="search">
		<table class="show">
			<tbody>
				<tr>
					<th><spring:message code="productState_totalPv" /></th>
					<td>
						<form:input path="productState.totalPv" cssClass="number"/>
						<form:errors path="productState.totalPv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_totalUv" /></th>
					<td>
						<form:input path="productState.totalUv" cssClass="number"/>
						<form:errors path="productState.totalUv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_totalDl" /></th>
					<td>
						<form:input path="productState.totalDl" cssClass="number"/>
						<form:errors path="productState.totalDl" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="productState_dailyPv" /></th>
					<td>
						<form:input path="productState.dailyPv" cssClass="number"/>
						<form:errors path="productState.dailyPv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_dailyUv" /></th>
					<td>
						<form:input path="productState.dailyUv" cssClass="number"/>
						<form:errors path="productState.dailyUv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_dailyDl" /></th>
					<td>
						<form:input path="productState.dailyDl" cssClass="number"/>
						<form:errors path="productState.dailyDl" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="productState_weekPv" /></th>
					<td>
						<form:input path="productState.weekPv" cssClass="number"/>
						<form:errors path="productState.weekPv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_weekUv" /></th>
					<td>
						<form:input path="productState.weekUv" cssClass="number"/>
						<form:errors path="productState.weekUv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_weekDl" /></th>
					<td>
						<form:input path="productState.weekDl" cssClass="number"/>
						<form:errors path="productState.weekDl" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="productState_monthPv" /></th>
					<td>
						<form:input path="productState.monthPv" cssClass="number"/>
						<form:errors path="productState.monthPv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_monthUv" /></th>
					<td>
						<form:input path="productState.monthUv" cssClass="number"/>
						<form:errors path="productState.monthUv" cssClass="error"/>
					</td>
					<th><spring:message code="productState_monthDl" /></th>
					<td>
						<form:input path="productState.monthDl" cssClass="number"/>
						<form:errors path="productState.monthDl" cssClass="error"/>
					</td>
				</tr>
			</tbody>
			<tfoot>
				<tr>
					<td colspan="6">
						<spring:message code="crud.search.button" var="searchButton"/>
						<input type="submit" class="button button-search" value="${searchButton}"/>
					</td>
				</tr>
			</tfoot>
		</table>
	</form:form>
	<jsp:include page="/domain/productstate/list" />
</body>
</html>