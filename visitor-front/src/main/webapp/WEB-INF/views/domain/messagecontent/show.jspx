<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" 
xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" 
xmlns:form="http://www.springframework.org/tags/form" xmlns:security="http://www.springframework.org/security/tags" 
xmlns:spring="http://www.springframework.org/tags" version="2.0">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />
<head>
	<title><spring:message code="messageContent" />: <spring:message code="crud.functionalities.show" /></title>
</head>
<body>

	<a href="${pageContext.request.contextPath}/domain/messagecontent/search" class="ajaxy button button-search"><spring:message code="messageContent" /></a>
	<span class="ui-state-highlight button button-show"><spring:message code="crud.show.button" /></span>
	<a href="${pageContext.request.contextPath}/domain/messagecontent/update/${messageContent.primaryKey}" class="ajaxy button button-edit"><spring:message code="crud.edit.button" /></a>
	<a href="${pageContext.request.contextPath}/domain/messagecontent/delete/${messageContent.primaryKey}" class="modal button button-delete"><spring:message code="crud.delete.button" /></a>

	<table class="show">
		<tbody>
			<tr>
				<th><spring:message code="messageContent_qq" /></th>
				<td><spring:eval expression="messageContent.qq"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageContent_nickName" /></th>
				<td><spring:eval expression="messageContent.nickName"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageContent_mobileName" /></th>
				<td><spring:eval expression="messageContent.mobileName"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageContent_mobileNumber" /></th>
				<td><spring:eval expression="messageContent.mobileNumber"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageContent_content" /></th>
				<td><spring:eval expression="messageContent.content"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageContent_createDate" /></th>
				<td><spring:eval expression="messageContent.createDate"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageContent_status" /></th>
				<td><spring:eval expression="messageContent.status"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageContent_replyDate" /></th>
				<td><spring:eval expression="messageContent.replyDate"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageContent_replyBy" /></th>
				<td><spring:eval expression="messageContent.replyBy"/></td>
			</tr>
			<tr>
				<th><spring:message code="messageContent_messageType" /></th>
				<td><spring:eval expression="messageContent.messageType"/></td>
			</tr>
		</tbody>
	</table>
</body>
</html>