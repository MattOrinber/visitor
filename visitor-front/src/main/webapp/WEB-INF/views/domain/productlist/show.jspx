<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" 
xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" 
xmlns:form="http://www.springframework.org/tags/form" xmlns:security="http://www.springframework.org/security/tags" 
xmlns:spring="http://www.springframework.org/tags" xmlns:fmt="http://java.sun.com/jsp/jstl/fmt" version="2.0">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />	
<head>

<title><spring:message code="productList" />: <spring:message code="crud.functionalities.show" /></title>
<script type="text/javascript">
	function viewPic(url, event) {
		event.preventDefault();
		event.stopPropagation();
		window.open(url, "picwin","height=480, width=320,toolbar=no ,scrollbars=" + scroll + ",menubar=no");
	}
	
	$(document).ready(function() {
	
		loadState();
		
		$("#edit_state").click(function(){
			updateState();
		});
	});
	
	function loadState() {
		$("#state").load("${pageContext.request.contextPath}/domain/productstate/show/${productList.primaryKey}" + " #result")
	}
	
	function updateState() {
		$("#state").load("${pageContext.request.contextPath}/domain/productstate/update/${productList.primaryKey}" + " #update",
			function() {
				setupPage();
			});
	}
	
</script>
</head>
<body>

	<a href="${pageContext.request.contextPath}/domain/productlist/search" class="ajaxy button button-search"><spring:message code="productList" /></a>
	<span class="ui-state-highlight button button-show"><spring:message code="crud.show.button" /> </span>
	<a href="${pageContext.request.contextPath}/domain/productlist/update/${productList.primaryKey}" class="ajaxy button button-edit"><spring:message code="crud.edit.button" /> </a>
	<c:if test="${productList.downStatus==0}">
		<a href="${pageContext.request.contextPath}/domain/productlist/delete/${productList.primaryKey}?type=1" class="modal button button-delete"><spring:message code="productList_status_disable" /></a>
	</c:if>
	<c:if test="${productList.downStatus==1}">
		<a href="${pageContext.request.contextPath}/domain/productlist/delete/${productList.primaryKey}?type=1" class="modal button button-delete"><spring:message code="productList_status_enable" /></a>
	</c:if>
	<a href="${pageContext.request.contextPath}/domain/productlist/preview/recommend/${productList.primaryKey}" class="ajaxy button button-show"><spring:message code="productList_preview_recommend" /> </a>

    <a href="${pageContext.request.contextPath}/domain/productlist/label/addlabels/${productList.primaryKey}" class="ajaxy button button-show"> <spring:message code="productList_addLabel"/> </a>
    <table class="show">
		<tbody>
			<tr>
				<th>
					<spring:message code="productList_productId" />
				</th>
				<td style="width:35%">
					<c:out value="${productList.productId}" />
				</td>
				<th>
					<spring:message code="productList_name" />
				</th>
				<td>
					<c:out value="${productList.name}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_category" />
				</th>
				<td>
					<c:out value="${productList.category.name}" />
				</td>
				<th>
					<spring:message code="productList_productVersion" />
				</th>
				<td>
					<c:out value="${productList.productVersion}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_billingType" />
				</th>
				<td>
					<c:out value="${productList.billingType.name}" />
				</td>
				<th>
					<spring:message code="productList_price" />
				</th>
				<td>
					<c:out value="${productList.price}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_tagLine" />
				</th>
				<td>
					<c:out value="${productList.tagLine}" />
				</td>
				<th>
					<spring:message code="productList_label" />
				</th>
				<td>
                    <c:forEach items="${productList.labels}" var="label" varStatus="_st">
                        <c:out value="${label.name}" /> <c:out value=";" />
                            </c:forEach>

				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_downStatus" />
				</th>
				<td>
					<c:if test="${productList.downStatus==0}"><spring:message code="productList_status_enable" /></c:if>
					<c:if test="${productList.downStatus==1}"><spring:message code="productList_status_disable" />（<spring:message code="productList_downReason" />：${productList.downReason}）</c:if>
				</td>
				<th>
					<spring:message code="productList_starLevel" />
				</th>
				<td>
					<c:out value="${productList.starLevel}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_description" />
				</th>
				<td colspan="3">
					<c:out value="${productList.description}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_manual_description" />
				</th>
				<td colspan="3">
					<c:out value="${productList.manualDescription}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_importBy" />
				</th>
				<td>
					<c:out value="${productList.importBy}" />
				</td>
				<th>
					<spring:message code="productList_productSource" />
				</th>
				<td>
					<c:out value="${productList.productSource.name}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_productSourceId" />
				</th>
				<td>
					<c:out value="${productList.productSourceId}" />
				</td>
				<th>
					<spring:message code="productList_productSourceName" />
				</th>
				<td>
					<c:out value="${productList.productSource.name}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_developer" />
				</th>
				<td>
					<c:out value="${productList.developer}" />
				</td>
				<th>
					<spring:message code="productList_operator" />
				</th>
				<td>
					${productList.operator.name}
				</td>
			</tr>

			<tr>
				<th>
					<spring:message code="productList_operationModel" />
				</th>
				<td>
					<c:out value="${productList.operationModel.name}" />
				</td>
				<th>
					<spring:message code="productList_cooperationModel" />
				</th>
				<td>
					<c:out value="${productList.cooperationModel.name}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_modDate" />
				</th>
				<td>
					<fmt:formatDate value="${productList.modDate}" type="date" pattern="yyyy-MM-dd"/>
				</td>
				<th>
					<spring:message code="productList_modBy" />
				</th>
				<td>
					<c:out value="${productList.modBy}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_createDate" />
				</th>
				<td>
					<fmt:formatDate value="${productList.createDate}" type="date" pattern="yyyy-MM-dd"/>
				</td>
				<th>
					<spring:message code="productList_createBy" />
				</th>
				<td>
					<c:out value="${productList.createBy}" />
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_merchant" />
				</th>
				<td>
					<c:out value="${productList.merchant.name}" />
				</td>
				<th>
					<spring:message code="productList_safeType" />
				</th>
				<td>
					<c:if test="${productList.safeType == 0}"><spring:message code="productList_safeType_unknow" /></c:if>
					<c:if test="${productList.safeType == 1}"><font color="green"><spring:message code="productList_safeType_safe" /></font></c:if>
					<c:if test="${productList.safeType == 2}"><font color="red"><spring:message code="productList_safeType_unsafe" /></font></c:if>
				</td>
			</tr>
			<tr>
				<th>
					<spring:message code="productList_productType" />
				</th>
				<td colspan="3">
					<c:if test="${productList.productType == 0}"><spring:message code="productList_productType_downloadable" /></c:if>
					<c:if test="${productList.productType == 1}"><spring:message code="productList_productType_pageGame" /></c:if>
				</td>

			</tr>
            <tr>
                <th>
                    <spring:message code="productList_miniLab" />
                </th>
                <td>
                    <c:if test="${productList.miniLab == 1}"><spring:message code="productList_git" /></c:if>
                    <c:if test="${productList.miniLab == 2}"><spring:message code="productList_new" /></c:if>
                    <c:if test="${productList.miniLab == 3}"><spring:message code="productList_hot" /></c:if>
                    <c:if test="${productList.miniLab == 4}"><spring:message code="productList_firstissue" /></c:if>
                    <c:if test="${productList.miniLab == 5}"><spring:message code="productList_recommend" /></c:if>
                    <c:if test="${productList.miniLab == 6}"><spring:message code="productList_classics" /></c:if>
                </td>


            </tr>
		</tbody>
		<tfoot>
			<tr>
				<td colspan="4">
					<a href="${pageContext.request.contextPath}/domain/init/redis/product/${productList.productId}"
						class="modal button button-search"><spring:message code="crud.redis.publish.button" />
					</a>
				</td>
			</tr>
		</tfoot>
	</table>
	<!-- 产品详细信息 -->
	<span id="tab_folder" class="ui-state-highlight button"><spring:message code="productList_detail" /></span>
	<!--only show when the product has at least one file resource-->
	<c:if test="${canCreate}" >
		<a href="${pageContext.request.contextPath}/domain/productdetail/create?productId=${productList.primaryKey}" class="button button-edit">
			<spring:message code="productList_detail_create" />
		</a>
	</c:if>
	<c:if test="${productDetails != null}" >
		<table class="list">
			<thead>
				<tr>
					<th style="width:20%">
						<spring:message code="productList_manual_description" />
					</th>
					<th style="width:10%">
						<spring:message code="productList_miniLab" />
					</th>
					<th style="width:10%">
						<spring:message code="picture" />
					</th>
					<th style="width:20%">
						<spring:message code="productList_recommend_storage" />
					</th>
					<th style="width:20%">
						<spring:message code="productList_site" />
					</th>
					<th style="width:20%"><c:out value=" " /></th>
				</tr>
			</thead>
			<tbody>
				<c:forEach items="${productDetails}" var="productDetail" varStatus="_st">
					<tr>
						<td style="word-break:break-all">
							${productDetail.manualDescription}
						</td>
						<td>
							<c:if test="${productDetail.miniLab == 1}"><spring:message code="productList_miniLab_hot" /></c:if>
							<c:if test="${productDetail.miniLab == 2}"><spring:message code="productList_miniLab_new" /></c:if>
							<c:if test="${productDetail.miniLab == 3}"><spring:message code="productList_miniLab_olm" /></c:if>
							<c:if test="${productDetail.miniLab == 4}"><spring:message code="productList_miniLab_4" /></c:if>
							<c:if test="${productDetail.miniLab == 5}"><spring:message code="productList_miniLab_5" /></c:if>
							<c:if test="${productDetail.miniLab == null || productDetail.miniLab == ''}"><spring:message code="NULL" /></c:if>
						</td>
						<td>
						    <c:if test="${productDetail.picPath != null}">
						        <img src="${picDomain}${productDetail.picPath}" />
						    </c:if>
						    <c:if test="${productDetail.picPath == null}">
						        <spring:message code="NULL" />
						    </c:if>
						</td>
						<td>
							<c:if test="${productDetail.recommendStorage == 1}"><spring:message code="TRUE" /></c:if>
							<c:if test="${productDetail.recommendStorage == 0}"><spring:message code="FALSE" /></c:if>
						</td>
						<td>
							<c:out value="${productDetail.site.name}" />
						</td>
						<td>
							<a href="${pageContext.request.contextPath}/domain/productdetail/update/${productDetail.primaryKey}" class="button button-edit"><spring:message code="crud.edit.button" /></a>
							<a href="${pageContext.request.contextPath}/domain/productdetail/delete/${productDetail.primaryKey}" class="button button-delete modal"><spring:message code="crud.delete.button" /></a>
						</td>
					</tr>
				</c:forEach>
			</tbody>
		</table>
	</c:if>
	<!-- 产品资源文件 -->
	<c:if test="${productList.productType == 0}" >
		<span id="tab_folder" class="ui-state-highlight button"><spring:message code="productList_files" /></span>
		<a href="${pageContext.request.contextPath}/domain/productfile/create?pid=${productList.primaryKey}" class="button button-create"><spring:message code="crud.functionalities.create" /> </a>
		<!--only show when the product has at least one file resource-->
		<c:if test="${productFiles!=null and not empty productFiles}" >
			<a href="${pageContext.request.contextPath}/domain/productlist/safetype/${productList.primaryKey}" class="button button-edit modal"><spring:message code="crud.edit.button" /><spring:message code="productList_safeType" /></a>
		</c:if>
		<table class="list">
			<thead>
				<tr>
					<th>
						<spring:message code="productFile_fileName" />
					</th>
					<th style="width:8%">
						<spring:message code="productFile_status" />
					</th>
					<th style="width:8%">
						<spring:message code="productFile_platform" />
					</th>
					<th style="width:6%">
						<spring:message code="productFile_resolution" />
					</th>
					<th style="width:6%">
						<spring:message code="productFile_fileSuffix" />
					</th>
					<th style="width:6%">
						<spring:message code="productFile_dlCount" />
					</th>
					<th style="width:6%">
						<spring:message code="productFile_safeType" />
					</th>
					<th style="width:6%">
						<spring:message code="productFile_autoScan" />
					</th>
					<th style="width:10%">
						<spring:message code="productFile_modBy" />
					</th>
					<th>
						<spring:message code="productFile_modDate" />
					</th>
					<th style="width:23%"><c:out value=" " /></th>
				</tr>
			</thead>
			<tbody>
				<c:forEach items="${productFiles}" var="productFile" varStatus="_st">
					<tr>
						<td>
							<c:if test="${productFile.fileName != null and productFile.fileName != ''}">
							<a href="${pageContext.request.contextPath}/domain/productfile/show/${productFile.primaryKey}"><c:out value="${productFile.fileName}" /></a>
							</c:if>
							<c:if test="${productFile.fileName == null or productFile.fileName == ''}">
							<a href="${pageContext.request.contextPath}/domain/productfile/show/${productFile.primaryKey}"><c:out value="${productFile.fileUrl}" /></a>
							</c:if>
						</td>
						<td>
							<c:if test="${productFile.status == 0}"><font color="green">上线</font></c:if>
							<c:if test="${productFile.status == 1}"><font color="red">下线</font></c:if>
						</td>
						<td>
							${productFile.os.name} <c:if test="${productFile.os.name != productFile.platform.name}">${productFile.platform.name} </c:if>${productFile.platformVersion.name}
						</td>
						<td>
							${productFile.resolution.name}
						</td>
						<td>
							<c:out value="${productFile.fileSuffix}" />
						</td>
						<td>
							<c:out value="${productFile.dlCount}" />
						</td>
						<td>
							<c:if test="${productFile.safeType == 0}"><spring:message code="productFile_safeType_unkown" /></c:if>
							<c:if test="${productFile.safeType == 1}"><font color="green"><spring:message code="productFile_safeType_safe" /></font></c:if>
							<c:if test="${productFile.safeType == 2}"><font color="orange"><spring:message code="productFile_safeType_lowrisk" /></font></c:if>
							<c:if test="${productFile.safeType == 3}"><font color="orange"><spring:message code="productFile_safeType_midrisk" /></font></c:if>
							<c:if test="${productFile.safeType == 4}"><font color="red"><spring:message code="productFile_safeType_virus" /></font></c:if>
						</td>
						<td>
							<c:if test="${productFile.autoScan == 0}"><spring:message code="OPEN" /></c:if>
							<c:if test="${productFile.autoScan == 1}"><font color="red"><spring:message code="CLOSE" /></font></c:if>
						</td>
						<td>
							<c:out value="${productFile.modBy}" />
						</td>
						<td>
							<fmt:formatDate value="${productFile.modDate}" type="date" pattern="yyyy-MM-dd"/>
						</td>
						<td>
							<a href="${pageContext.request.contextPath}/domain/productfile/replace/${productFile.primaryKey}" class="button"><spring:message code="productFile_replace" /></a>
							<c:choose>
								<c:when test="${!empty productFile.filePath}">
									<a href="${downloadDomain}${productFile.filePath}" class="button"><spring:message code="productFile_download" /></a>
								</c:when>
								<c:otherwise>
									<a href="${productFile.fileUrl}" class="button"><spring:message code="productFile_download" /></a>
								</c:otherwise>
							</c:choose>
							<c:if test="${productFile.status == 0}"><a href="${pageContext.request.contextPath}/domain/productfile/delete/${productFile.primaryKey}" class="button button-delete modal"><spring:message code="crud.delete.button" /></a></c:if>
							<c:if test="${productFile.status == 1}"><a href="${pageContext.request.contextPath}/domain/productfile/online/${productFile.primaryKey}" class="button button-edit modal">上线</a></c:if>
						</td>
					</tr>
				</c:forEach>
			</tbody>
		</table>
	</c:if>
	<c:if test="${productList.productType == 1}" >
		<span id="tab_folder" class="ui-state-highlight button"><spring:message code="productList_files" /></span>
		<a href="${pageContext.request.contextPath}/domain/productfile/createpagegame?pid=${productList.primaryKey}" class="button button-create"><spring:message code="crud.functionalities.create" /> </a>
		<table class="list">
			<thead>
				<tr>
					<th style="width:10%">
						<spring:message code="productFile_SoupengName" />
					</th>
					<th>
						<spring:message code="productFile_PageGameUrl" />
					</th>
					<th>
						<spring:message code="productFile_SoupengId" />
					</th>
					<th style="width:10%">
						<spring:message code="productFile_modBy" />
					</th>
					<th style="width:10%">
						<spring:message code="productFile_modDate" />
					</th>
					<th style="width:10%"><c:out value=" " /></th>
				</tr>
			</thead>
			<tbody>
				<c:forEach items="${productFiles}" var="productFile" varStatus="_st">
					<tr>
						<td>
							<a href="${pageContext.request.contextPath}/domain/productfile/showpagegame/${productFile.primaryKey}"><c:out value="${productFile.fileName}" /></a>
						</td>
						<td>
							<c:if test="${productFile.status == 0}"><font color="green"><c:out value="${productFile.fileUrl}" /></font></c:if>
							<c:if test="${productFile.status == 1}"><c:out value="${productFile.fileUrl}" /></c:if>
						</td>
						<td>
							<c:if test="${productFile.status == 0}"><font color="green"><c:out value="${productFile.filePath}" /></font></c:if>
							<c:if test="${productFile.status == 1}"><c:out value="${productFile.filePath}" /></c:if>
						</td>
						<td>
							<c:out value="${productFile.modBy}" />
						</td>
						<td>
							<fmt:formatDate value="${productFile.modDate}" type="date" pattern="yyyy-MM-dd"/>
						</td>
						<td>
							<a href="${pageContext.request.contextPath}/domain/productfile/updatepagegame/${productFile.primaryKey}" class="ajaxy button button-edit"><spring:message code="crud.edit.button" /> </a>
						</td>
					</tr>
				</c:forEach>
			</tbody>
		</table>
	</c:if>
	<!-- 产品图片 -->
	<span id="tab_folder" class="ui-state-highlight button"><spring:message code="productList_productPic" /></span>
	<a
		href="${pageContext.request.contextPath}/domain/productpic/createscr?proId=${productList.primaryKey}"
		class="button button-create"><spring:message code="crud.functionalities.create" /> </a>
	<table class="list">
		<thead>
			<tr>
				<th>
					<spring:message code="productPic_pic" />
				</th>
				<th>
					<spring:message code="productPic_picPath" />
				</th>
				<th>
					<spring:message code="productPic_picSize" />
				</th>
				<th>
					<spring:message code="productPic_picType" />
				</th>
				<th style="width:25%"><c:out value=" " /></th>
			</tr>
		</thead>
		<tbody>
			<c:forEach items="${productPics}" var="productPic" varStatus="_st">
				<tr>
					<td>
						<img alt="缩略图" src="${picDomain }${productPic.picPath}" width="50" height="50" />
					</td>
					<td>
						<c:out value="${productPic.picPath}" />
					</td>
					<td>
						<c:out value="${productPic.picSize}" />
					</td>
					<td>
						<!-- 0-封面图；1-图标；2-截图 -->
						<c:choose>
							<c:when test="${productPic.picType == 0}">
								封面图
							</c:when>
							<c:when test="${productPic.picType == 1}">
								图标
							</c:when>
							<c:when test="${productPic.picType == 2}">
								截图
							</c:when>
						</c:choose>
					</td>
					<td>
						<c:if test="${productPic.picType == 0||productPic.picType == 1}">
							<a href="${pageContext.request.contextPath}/domain/productpic/replace?ppId=${productPic.productPicId}" class="button"><spring:message code="productPic_replace" /> </a>
						</c:if>
						<c:if test="${productPic.picType != 1}">
							<a href="#" onclick="viewPic('${picDomain }${productPic.picPath}',event)" class="button"><spring:message code="productPic_view" /> </a>
						</c:if>
						<c:if test="${productPic.picType == 2}">
							<a href="${pageContext.request.contextPath}/domain/productpic/delete/${productPic.primaryKey}" class="button button-delete modal"><spring:message code="crud.delete.button" /> </a>
						</c:if>
						<c:if test="${productPic.picType == 2}">
							<a href="${pageContext.request.contextPath}/domain/productpic/tocover/${productPic.primaryKey}" class="button"><spring:message code="productPic_tocover" /> </a>
						</c:if>


					</td>
				</tr>
			</c:forEach>
		</tbody>
	</table>

	<!-- 频道绑定 -->
	<span id="tab_folder" class="ui-state-highlight button"><spring:message code="productSiteFolder" /></span>
	<a href="${pageContext.request.contextPath}/domain/productsitefolder/create?productId=${productList.primaryKey}" class="button button-create"><spring:message code="productSiteFolder_bind" /> </a>
	<table class="list">
		<thead>
			<tr>
				<th>
					<spring:message code="productSiteFolder_site" />
				</th>
				<th>
					<spring:message code="productSiteFolder_folder" />
				</th>
				<th>
					<spring:message code="productSiteFolder_sortOrder" />
				</th>
				<th><c:out value=" " /></th>
				<th><c:out value=" " /></th>
			</tr>
		</thead>
		<tbody>
			<c:forEach items="${folderList}" var="productFolder" varStatus="_st">
				<tr>
					<td>
						<c:out value="${productFolder.site.name}" />
					</td>
					<td>
						<c:out value="${productFolder.folder.path}" />
					</td>
					<td><c:out value="${productFolder.sortOrder}" /></td>
					<td>
						<c:if test="${productFolder.folder.status == 1}"><spring:message code="folder_status_no" /></c:if>
					</td>
					<td>
						<a href="${pageContext.request.contextPath}/domain/productsitefolder/update/${productFolder.primaryKey}" class="button button-edit miniature"><spring:message code="crud.edit.button"/></a>

						<a href="${pageContext.request.contextPath}/domain/productsitefolder/delete/${productFolder.primaryKey}"
							class="button button-delete modal"><spring:message code="productSiteFolder_unbind" /> </a>
					</td>
				</tr>
			</c:forEach>
		</tbody>
	</table>

	<!-- 产品推荐 -->
	<span id="tab_folder" class="ui-state-highlight button"><spring:message code="productContainer" /></span>
	<a
		href="${pageContext.request.contextPath}/domain/productcontainer/create/${productList.primaryKey}/1"
		class="button button-create"><spring:message code="crud.functionalities.create" /> </a>
	<table class="list">
		<thead>
			<tr>
				<th style="width:10%">
					<spring:message code="productContainer_site" />
				</th>
				<th style="width:17%">
					<spring:message code="productContainer_folder" />
				</th>
				<th style="width:18%">
					<spring:message code="productContainer_container" />
				</th>
				<th>
					<spring:message code="productContainer_page" />
				</th>
				<th>
					<spring:message code="productContainer_sortOrder" />
				</th>
				<th>
					<spring:message code="productContainer_createBy" />
				</th>
				<th>
					<spring:message code="productContainer_createDate" />
				</th>
				<th><c:out value=" " /></th>
			</tr>
		</thead>
		<tbody>
			<c:forEach items="${recommandList}" var="recommand" varStatus="_st">
				<tr>
					<td>
						<a
							href="${pageContext.request.contextPath}/domain/site/show/${recommand.site.primaryKey}">
							<c:out value="${recommand.site.name}" /> </a>
					</td>
					<td>
						<a
							href="${pageContext.request.contextPath}/domain/folder/show/${recommand.folder.primaryKey}">
							<c:out value="${recommand.folder.name}" /> </a>
					</td>
					<td>
						<a
							href="${pageContext.request.contextPath}/domain/recommandcontainer/show/${recommand.container.primaryKey}">
							<c:out value="${recommand.container.name}" /> </a>
					</td>
					<td>
						<c:out value="${recommand.page.name}" />
					</td>

					<td>
						<c:out value="${recommand.sortOrder}" />
					</td>
					<td>
						<c:out value="${recommand.createBy}" />
					</td>
					<td>
						<fmt:formatDate value="${recommand.createDate}" type="date" pattern="yyyy-MM-dd" />
					</td>
					<td>
						<a
							href="${pageContext.request.contextPath}/domain/productcontainer/update/${recommand.primaryKey}" class="button">
							<spring:message code="crud.edit.button" /> </a>
						<a  href="${pageContext.request.contextPath}/domain/productcontainer/delete/${recommand.primaryKey}"
							class="button button-delete modal miniature"><spring:message code="crud.delete.button" /> </a>
					</td>
				</tr>
			</c:forEach>
		</tbody>
	</table>

	<!-- 产品数据 -->
	<span class="ui-state-highlight button"><spring:message code="productState" /></span>
	<a id="edit_state" class="button button-edit"><spring:message code="crud.edit.button" /> </a>
	<div id="state">
	</div>

</body>
</html>