<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" 
xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" 
xmlns:form="http://www.springframework.org/tags/form" xmlns:security="http://www.springframework.org/security/tags" 
xmlns:spring="http://www.springframework.org/tags" version="2.0">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />
<head>
	<title><spring:message code="recommendRuleAcross" />: <spring:message code="crud.functionalities.example" /></title>
</head>
<body>
	<span class="ui-state-highlight button button-search"><spring:message code="recommendRuleAcross" /></span>
	<a href="${pageContext.request.contextPath}/domain/recommendruleacross/create/?type=1" class="ajaxy button button-create"><spring:message code="crud.functionalities.create" /><spring:message code="recommendRuleAcross_folder_recommend" /></a>
	<a href="${pageContext.request.contextPath}/domain/recommendruleacross/create/?type=2" class="ajaxy button button-create"><spring:message code="crud.functionalities.create" /><spring:message code="recommendRuleAcross_product_recommend" /></a>
	<form:form action="${pageContext.request.contextPath}/domain/recommendruleacross/list" modelAttribute="recommendRuleAcrossSearchForm" class="search">
		<table class="show">
			<tbody>
				<tr>
					<th><spring:message code="recommendRuleAcross_ruleId" /></th>
					<td>
						<form:input path="recommendRuleAcross.ruleId" cssClass="number"/>
						<form:errors path="recommendRuleAcross.ruleId" cssClass="error"/>
					</td>
					<th><spring:message code="recommendRuleAcross_type" /></th>
					<td>
						<form:radiobutton path="recommendRuleAcross.Type" value="" checked="checked"/><spring:message code="ANY" />
						<form:radiobutton path="recommendRuleAcross.Type" value="1"/><spring:message code="recommendRuleAcross_type_1" />
						<form:radiobutton path="recommendRuleAcross.Type" value="2" /><spring:message code="recommendRuleAcross_type_2" />
						<form:errors path="recommendRuleAcross.Type" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="recommendRuleAcross_serviceId" /></th>
					<td>
						<form:input path="recommendRuleAcross.serviceId" cssClass="number"/>
						<form:errors path="recommendRuleAcross.serviceId" cssClass="error"/>
					</td>
					<th><spring:message code="recommendRuleAcross_serviceSiteId" /></th>
					<td>
						<form:input path="recommendRuleAcross.serviceSiteId" cssClass="number"/>
						<form:errors path="recommendRuleAcross.serviceSiteId" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="recommendRuleAcross_serviceFolderId" /></th>
					<td>
						<form:input path="recommendRuleAcross.serviceFolderId" cssClass="number"/>
						<form:errors path="recommendRuleAcross.serviceFolderId" cssClass="error"/>
					</td>
					<th><spring:message code="recommendRuleAcross_serviceProductId" /></th>
					<td>
						<form:input path="recommendRuleAcross.serviceProductId" cssClass="number"/>
						<form:errors path="recommendRuleAcross.serviceProductId" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="recommendRuleAcross_name" /></th>
					<td colspan="3">
						<form:input path="recommendRuleAcross.name"/>
						<form:errors path="recommendRuleAcross.name" cssClass="error"/>
					</td>
				</tr>
			</tbody>
			<tfoot>
				<tr>
					<td colspan="6">
						<spring:message code="crud.search.button" var="searchButton"/>
						<input type="submit" class="button button-search" value="${searchButton}"/>
					</td>
				</tr>
			</tfoot>
		</table>
	</form:form>
	<jsp:include page="/domain/recommendruleacross/list" />
</body>
</html>