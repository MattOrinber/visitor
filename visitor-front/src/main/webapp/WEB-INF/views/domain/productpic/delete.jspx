<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" 
xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" 
xmlns:form="http://www.springframework.org/tags/form" xmlns:security="http://www.springframework.org/security/tags" 
xmlns:spring="http://www.springframework.org/tags" version="2.0">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />
<head>
	<title><spring:message code="productPic" />: <spring:message code="crud.functionalities.delete" /></title>
</head>
<body>
	<form:form action="${pageContext.request.contextPath}/domain/productpic/delete/${productPic.primaryKey}" method="POST">
		<p>
			<spring:message code="entity_delete_confirm"/>
			<br/>
			<span class="alt"><spring:eval expression="productPic.picPath" /><br/></span>
			<span class="alt"><img src="${picDomain}${productPic.picPath}" width="96px;"/></span>
		</p>
		<spring:message code="crud.deletion.confirmation" var="confirmation"/>
		<input type="submit" value="${confirmation}" class="button button-delete" />
	</form:form>
</body>
</html>