<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:security="http://www.springframework.org/security/tags" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">

<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />

<head>
	<title><spring:message code="recommandContainer" />: <spring:message code="crud.functionalities.example" /></title>
</head>
<body>
	<span class="ui-state-highlight button button-search"><spring:message code="recommandContainer" /></span> 
	<a href="${pageContext.request.contextPath}/domain/recommandcontainer/create" class="button button-create"><spring:message code="crud.functionalities.create" /></a> 
	<a href="${pageContext.request.contextPath}/domain/init/redis/productContainers"
		class="modal button button-search"><spring:message code="crud.redis.publish.allcontainer.button" />
	</a>
	<form:form action="${pageContext.request.contextPath}/domain/recommandcontainer/list" modelAttribute="recommandContainerSearchForm" class="search">
		<table class="show">
			<tbody>
				<tr>
					<th><spring:message code="recommandContainer_name" /></th>
					<td>
						<form:input path="recommandContainer.name"/>
						<form:errors path="recommandContainer.name" cssClass="error"/>
					</td>
					<th><spring:message code="recommandContainer_description" /></th>
					<td>
						<form:input path="recommandContainer.description" size="40"/>
						<form:errors path="recommandContainer.description" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="recommandContainer_createBy" /></th>
					<td>
						<form:input path="recommandContainer.createBy"/>
						<form:errors path="recommandContainer.createBy" cssClass="error"/>
					</td>
					<th><spring:message code="recommandContainer_createDate" /></th>
					<td>
						<label for="createDateRange.fromDate"><spring:message code="search.date.from" /></label>
						<form:input path="createDateRange.fromDate" cssClass="datepicker"/>
						<label for="createDateRange.toDate"><spring:message code="search.date.to" /></label>
						<form:input path="createDateRange.toDate" cssClass="datepicker"/>
						<form:errors path="recommandContainer.createDate" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="recommandContainer_modBy" /></th>
					<td>
						<form:input path="recommandContainer.modBy"/>
						<form:errors path="recommandContainer.modBy" cssClass="error"/>
					</td>
					<th><spring:message code="recommandContainer_modDate" /></th>
					<td>
						<label for="modDateRange.fromDate"><spring:message code="search.date.from" /></label>
						<form:input path="modDateRange.fromDate" cssClass="datepicker"/>
						<label for="modDateRange.toDate"><spring:message code="search.date.to" /></label>
						<form:input path="modDateRange.toDate" cssClass="datepicker"/>
						<form:errors path="recommandContainer.modDate" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="recommandContainer_status" /></th>
					<td>
						<form:radiobutton path="recommandContainer.status" value="" checked="checked"/><spring:message code="recommandContainer_status_all" />
						<form:radiobutton path="recommandContainer.status" value="0" /><spring:message code="recommandContainer_status_yes" />
						<form:radiobutton path="recommandContainer.status" value="1" /><spring:message code="recommandContainer_status_no" />
						<form:errors path="recommandContainer.status" cssClass="error"/>
					</td>
				</tr>
			</tbody>
			<tfoot>
				<tr>
					<td colspan="4">
						<spring:message code="crud.search.button" var="btnMsg" />
						<input type="submit" class="button button-search" value="${btnMsg}"/>
					</td>
				</tr>
			</tfoot>
		</table>
	</form:form>

	<spring:message code="crud.functionalities.list" arguments="${title}" var="title_msg" />
	<jsp:include page="/domain/recommandcontainer/list" />
</body>
</html>