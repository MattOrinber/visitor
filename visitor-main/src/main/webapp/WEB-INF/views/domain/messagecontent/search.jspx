<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" 
xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" 
xmlns:form="http://www.springframework.org/tags/form" xmlns:security="http://www.springframework.org/security/tags" 
xmlns:spring="http://www.springframework.org/tags" version="2.0">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />
<head>
	<title><spring:message code="messageContent" />: <spring:message code="crud.functionalities.example" /></title>
</head>
<body>
	<span class="ui-state-highlight button button-search"><spring:message code="messageContent" /></span> 
	<a href="${pageContext.request.contextPath}/domain/messagecontent/create" class="button button-create"><spring:message code="crud.functionalities.create" /></a> 
	
	<form:form action="${pageContext.request.contextPath}/domain/messagecontent/list" modelAttribute="messageContentSearchForm" class="search">
		<table class="show">
			<tbody>
				<tr>
					<th><spring:message code="messageContent_qq" /></th>
					<td>
						<form:input path="messageContent.qq" cssClass="number {maxlength: 20}" maxlength="20"/>
						<form:errors path="messageContent.qq" cssClass="error"/>
					</td>
					<th><spring:message code="messageContent_nickName" /></th>
					<td>
						<form:input path="messageContent.nickName"/>
						<form:errors path="messageContent.nickName" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="messageContent_mobileName" /></th>
					<td>
						<form:input path="messageContent.mobileName"/>
						<form:errors path="messageContent.mobileName" cssClass="error"/>
					</td>
					<th><spring:message code="messageContent_mobileNumber" /></th>
					<td>
						<form:input path="messageContent.mobileNumber"/>
						<form:errors path="messageContent.mobileNumber" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="messageContent_content" /></th>
					<td>
						<form:input path="messageContent.content"/>
						<form:errors path="messageContent.content" cssClass="error"/>
					</td>
					<th><spring:message code="messageContent_createDate" /></th>
					<td>
						<label for="createDateRange.fromDate"><spring:message code="search.date.from" /></label>
						<form:input path="createDateRange.fromDate" cssClass="datepicker"/>
						<label for="createDateRange.toDate"><spring:message code="search.date.to" /></label>
						<form:input path="createDateRange.toDate" cssClass="datepicker"/>
						<form:errors path="messageContent.createDate" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="messageContent_status" /></th>
					<td>
						<form:input path="messageContent.status"/>
						<form:errors path="messageContent.status" cssClass="error"/>
					</td>
					<th><spring:message code="messageContent_replyDate" /></th>
					<td>
						<label for="replyDateRange.fromDate"><spring:message code="search.date.from" /></label>
						<form:input path="replyDateRange.fromDate" cssClass="datepicker"/>
						<label for="replyDateRange.toDate"><spring:message code="search.date.to" /></label>
						<form:input path="replyDateRange.toDate" cssClass="datepicker"/>
						<form:errors path="messageContent.replyDate" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="messageContent_replyBy" /></th>
					<td>
						<form:input path="messageContent.replyBy"/>
						<form:errors path="messageContent.replyBy" cssClass="error"/>
					</td>
					<th><spring:message code="messageContent_messageType" /></th>
					<td>
						<form:input path="messageContent.messageType"/>
						<form:errors path="messageContent.messageType" cssClass="error"/>
					</td>
				</tr>
			</tbody>
			<tfoot>
				<tr>
					<td colspan="4">
						<spring:message code="crud.search.button" var="searchButton"/>
						<input type="submit" class="button button-search" value="${searchButton}"/>
					</td>
				</tr>
			</tfoot>
		</table>
	</form:form>
	<jsp:include page="/domain/messagecontent/list" />
</body>
</html>