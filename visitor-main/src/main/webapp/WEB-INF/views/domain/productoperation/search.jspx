<html xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" 
xmlns:decorator="http://www.opensymphony.com/sitemesh/decorator" xmlns:c="http://java.sun.com/jsp/jstl/core" 
xmlns:form="http://www.springframework.org/tags/form" xmlns:security="http://www.springframework.org/security/tags" 
xmlns:spring="http://www.springframework.org/tags" version="2.0">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html; charset=UTF-8" />
<head>
	<title><spring:message code="productOperation" />: <spring:message code="crud.functionalities.example" /></title>
</head>
<body>
	<span class="ui-state-highlight button button-search"><spring:message code="productOperation" /></span> 
	<a href="${pageContext.request.contextPath}/domain/productoperation/create" class="button button-create"><spring:message code="crud.functionalities.create" /></a> 
	<a href="${pageContext.request.contextPath}/domain/productoperation/showcase" class="button button-create"><spring:message code="crud.functionalities.showcase" /></a>

	<form:form action="${pageContext.request.contextPath}/domain/productoperation/list" modelAttribute="productOperationSearchForm" class="search">
		<table class="show">
			<tbody>
				<tr>
					<th><spring:message code="productOperation_productId" /></th>
					<td>
						<form:input path="productOperation.productId"/>
						<form:errors path="productOperation.productId" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="productOperation_type" /></th>
					<td>
						<form:input path="productOperation.type"/>
						<form:errors path="productOperation.type" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="productOperation_content" /></th>
					<td>
						<form:textarea path="productOperation.content"/>
						<form:errors path="productOperation.content" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="productOperation_createBy" /></th>
					<td>
						<form:input path="productOperation.createBy"/>
						<form:errors path="productOperation.createBy" cssClass="error"/>
					</td>
				</tr>
				<tr>
					<th><spring:message code="productOperation_createDate" /></th>
					<td>
						<label for="createDateRange.fromDate"><spring:message code="search.date.from" /></label>
						<form:input path="createDateRange.fromDate" cssClass="datepicker"/>
						<label for="createDateRange.toDate"><spring:message code="search.date.to" /></label>
						<form:input path="createDateRange.toDate" cssClass="datepicker"/>
						<form:errors path="productOperation.createDate" cssClass="error"/>
					</td>
				</tr>
			</tbody>
			<tfoot>
				<tr>
					<td colspan="2">
						<input type="submit" class="button button-search" value="<spring:message code="crud.search.button" />"/>
					</td>
				</tr>
			</tfoot>
		</table>
	</form:form>
	<jsp:include page="/domain/productoperation/list" />
</body>
</html>