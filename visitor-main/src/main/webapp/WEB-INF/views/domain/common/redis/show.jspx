<script type="text/javascript">
	$(document).ready(function() {
		setTabValue();
		$("#show_site").click();
	});
	
	function setTabValue() {
		$("span[id^=show_]").click(function() {
			setTabValueArea(this.id);
			resetAreaTabs(this.id);
		});
	}
	
	function setTabValueArea(name) {
		$("#dis_show_area").hide().html($("#" + name + "_area").html()).fadeIn();
		initRedis();
		$("#result").html('');
	}

	function resetAreaTabs(name) {
		$("span[id^=show_]").removeClass();
		$("span[id^=show_]").addClass("ajaxy button button-show ui-button ui-widget ui-state-default ui-corner-all ui-button-text-icon-primary");
		$("#" + name).removeClass();
		$("#" + name).addClass("ui-state-highlight button button-show ui-button ui-widget ui-state-default ui-corner-all ui-button-text-icon-primary");
		setupPage();
	}
	function initRedis() {
		$("input[type='button']").click(function(){
		
			var nm = $(this).attr("id");
			var type = $("#"+ nm +"_t").val();
			var hash = $("#"+ nm +"_h").html();
			var keypara = $("#"+ nm +"_hk").html();
			var para = $("#"+ nm +"_p").val();//产品ID的参数
			
			if (keypara != null &amp;&amp; keypara != '') {
				for (var i=1; i &lt; 10;i++) {
					var pa = $("#"+ nm +"_p" + i).val();
					if (pa != null &amp;&amp; pa != '') {
						keypara = keypara.replace("{" + i + "}",pa);
					} else {
						break;
					}
				}
			} else if(para != null &amp;&amp; para != ''){
				keypara = para;
			}
			
			if (type == 'kstr') {
				ajaxValue(keypara);
			} else if (type == 'kmap') {
				if (hash != null &amp;&amp; hash != '') {
					ajaxList(hash);
				} else {
					ajaxList(keypara);
				}
				
			} else if (type == 'hks') {
				if (keypara != null &amp;&amp; keypara != '') {
					ajaxString(hash, keypara);
				} else {
					alert('请输入参数');
				}
			} else if (type == 'hksc') {
				if (keypara != null &amp;&amp; keypara != '') {
					ajaxCompressString(hash, keypara);
				} else {
					alert('请输入参数');
				}
			} else if (type == 'kset') {
				var page = $("#"+ nm +"_page").val();
				var size = $("#"+ nm +"_size").val();
				
				ajaxSet(keypara , page , size);
			} else if (type == 'hsize') {
				ajaxCount(hash);
			} else if (type == 'hkzst') {//显示图书章节的ZSET
				if (keypara != null &amp;&amp; keypara != '') {
					var chapterId=$("#chapter_2_p").val();
					if(chapterId==""){
						alert('请输入参数');
					}else{
						ajaxZSet(keypara,chapterId);
					}
				} else {
					alert('请输入参数');
				}
			} 
			
			$("#keyvalue").html(hash + " " + keypara);
		})
	}
	
function ajaxList(key){
	$.ajax({
		type : "GET",
		url : "${pageContext.request.contextPath}/domain/redis/ajax/list?key=" + key,
		success : function(ss) {
			htmlList(ss);
		}
	})
}
	
function ajaxCompressString(key, para){
	$.ajax({
		type : "GET",
		dataType : "text",
		url : "${pageContext.request.contextPath}/domain/redis/ajax/compressstring?key=" + key + "&amp;para=" + para,
		success : function(ss) {
			htmlString(ss);
		}
	})
}
	
function ajaxString(key, para){
	$.ajax({
		type : "GET",
		url : "${pageContext.request.contextPath}/domain/redis/ajax/string?key=" + key + "&amp;para=" + para,
		success : function(ss) {
			htmlString(ss);
		}
	})
}

function ajaxValue(key){
	$.ajax({
		type : "GET",
		url : "${pageContext.request.contextPath}/domain/redis/ajax/value?key=" + key,
		success : function(ss) {
			htmlString(ss);
		}
	})
}
	
function ajaxSet(key, page, size){
	if (page == null || page == '') {
		page = 1;
		size = 50;
	}
	$.ajax({
		type : "GET",
		url : "${pageContext.request.contextPath}/domain/redis/ajax/set?key=" + key + "&amp;page=" + page + "&amp;size=" + size,
		success : function(ss) {
			htmlString(ss);
		}
	})
}

function ajaxCount(key){
	$.ajax({
		type : "GET",
		url : "${pageContext.request.contextPath}/domain/redis/ajax/count?key=" + key,
		success : function(ss) {
			htmlString(ss);
		}
	})
}

/*本方法用于获取章节列表*/
function ajaxZSet(key,chapid){
	//alert("ok"+key);
	$.ajax({
		type : "GET",
		url : "${pageContext.request.contextPath}/domain/redis/ajax/zset?key=" + key + "&amp;chapid=" + chapid,
		success : function(ss) {
			//alert("ok?"+ss);
			htmlString(ss);
		}
	})
}
		
function htmlString(str){
	$("#result").html('');
	if (str != null &amp;&amp; str != '') {
		$("#result").append("<textarea></textarea>").children("textarea").attr("style","width:100%").html(str);
	}
}

function htmlList(str){
	$("#result").html('');
	$("#result").append("<textarea></textarea>").children("textarea").attr("style","width:100%");
	if (str != null &amp;&amp; str.length &gt; 0) {
		for (var i = 0; i &lt; str.length; i++) {
			$("#result").children("textarea").append(str[i]);
		}
	}
}
</script>
